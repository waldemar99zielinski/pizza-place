   data.data.map((pizza) => {\n          return (\n            <PizzaPanel\n              key={pizza.pizza_code}\n              id={pizza.pizza_code}\n              name={pizza.name}\n              price_small={pizza.price_small}\n              price_big={pizza.price_big}\n              image={pizza.image}\n            />\n          );\n        })\n      )}\n    </div>\n  );\n};\n\nconst mapStateToProps = (state) => {\n  const data = state.pizza.data;\n  const isLoading = state.pizza.isLoading;\n  const error = state.pizza.error;\n  return { isLoading, data, error };\n};\n\nexport default connect(mapStateToProps)(PizzaOverView);\n","/home/waldemar/Documents/React/pizza-place/src/cart/OrderedPizzaList.js",[],"/home/waldemar/Documents/React/pizza-place/src/cart/OrderedPizzaListElement.js",[],"/home/waldemar/Documents/React/pizza-place/src/cart/OrderConfirmation.js",[],"/home/waldemar/Documents/React/pizza-place/src/cart/Cart.js",["118"],"/home/waldemar/Documents/React/pizza-place/src/popUp/PopUp.js",[],"/home/waldemar/Documents/React/pizza-place/src/redux/store.js",[],"/home/waldemar/Documents/React/pizza-place/src/redux/constants/actionTypes.js",[],"/home/waldemar/Documents/React/pizza-place/src/redux/reducers/index.js",[],"/home/waldemar/Documents/React/pizza-place/src/redux/reducers/popUp.js",["119"],"import { OPEN_POPUP, CLOSE_POPUP } from \"../constants/actionTypes\";\n\nconst initialState = { openedPopUpsIds: [] };\n\nconst reducer = (state = initialState, action) => {\n  switch (action.type) {\n    case OPEN_POPUP:\n      //   console.log(\"Reducer: popUp: OPEN_POPUP \", action.payload.id);\n      let isIdExisting = state.openedPopUpsIds.indexOf(action.payload.id) > -1;\n      let openedPopUpsIds = { ...state }.openedPopUpsIds;\n\n      if (!isIdExisting) {\n        return {\n          ...state,\n          openedPopUpsIds: [...state.openedPopUpsIds, action.payload.id],\n        };\n      }\n\n      return {\n        state,\n      };\n    case CLOSE_POPUP:\n    