r/Documents/React/pizza-place/src/App.js",["122"],"import \"./App.css\";\nimport { BrowserRouter as Router } from \"react-router-dom\";\nimport Navbar from \"./Navbar\";\nimport PizzaOverView from \"./pizzaPanel/PizzaOverView\";\nimport Cart from \"./cart/Cart\";\nimport React, { useState } from \"react\";\n\nfunction App() {\n  return (\n    <Router>\n      <div className=\"container\">\n        <Navbar />\n        <PizzaOverView />\n        <Cart />\n      </div>\n    </Router>\n  );\n}\n\nexport default App;\n","/home/waldemar/Documents/React/pizza-place/src/Navbar.js",[],"/home/waldemar/Documents/React/pizza-place/src/Panel.js",["123"],"/home/waldemar/Documents/React/pizza-place/src/pizzaPanel/PizzaPanel.js",["124"],"/home/waldemar/Documents/React/pizza-place/src/pizzaPanel/PizzaOverView.js",["125"],"import React, { useEffect } from \"react\";\n\n//redux\nimport { connect, useDispatch } from \"react-redux\";\nimport { fetchPizzas } from \"../redux/actions/pizza\";\n\nimport PizzaPanel from \"./PizzaPanel\";\nimport \"./PizzaPanel.css\";\nconst PizzaOverView = (props) => {\n  const dispatch = useDispatch();\n  useEffect(() => {\n    dispatch(fetchPizzas());\n  }, [dispatch]);\n\n  const { isLoading, data, error } = props;\n  const isEmpty = data.length === 0;\n  return (\n    <div className=\"panel-container\">\n      {isEmpty ? (\n        isLoading ? (\n          <h2>Loading...</h2>\n        ) : (\n          <h2>Empty.</h2>\n        )\n      ) : (\n        data.data.map((pizza) => {\n          return (\n            <PizzaPanel\n              key={pizza.pizza_code}\n              id={pizza.pizza_code}\n              name={pizza.name}\n              price_small={pizza.price_small}\n              price_big={pizza.price_big}\n              image={pizza.image}\n            />\n          );\n        })\n      )}\n    </div>\n  );\n};\n\nconst mapStateToProps = (state) => {\n  const data = state.pizza.data;\n  const isLoading = state.pizza.isLoading;\n  const error = state.pizza.error;\n  return { isLoading, data, error };\n};\n\nexport default connect(mapStateToProps)(PizzaOverView);\n","/home/waldemar/Documents/React/pizza-place/src/cart/OrderedPizzaList.js",[],"/home/waldemar/Documents/React/pizza-place/src/cart/OrderedPizzaListElement.js",["126"],"/home/waldemar/Documents/React/pizza-place/src/cart/OrderConfirmation.js",[],"/home/waldemar/Documents/React/pizza-place/src/cart/Cart.js",["127"],"import React, { useEffect } from \"react\";\nimport \"./Cart.css\";\n\n//componets\nimport PopUp from \"../popUp/PopUp\";\nimport OrderedPizzaList from \"./OrderedPizzaList\";\nimport OrderConfirmation from \"./OrderConfirmation\";\n\n//redux\nimport { connect } from \"react-redux\";\nimport { POPUP_ID_CART } from \"../redux/constants/popUpIds\";\nimport { getOrderedPizzas } from \"../redux/selectors/order\";\n\nconst Cart = (props) => {\n  return (\n    <PopUp title={\"YOUR CART\"} id={POPUP_ID_CART}>\n      <OrderedPizzaList pizzaOrders={props.data} />\n      <OrderConfirmation totalPrice={12.69 + \" USD\"} />\n    </PopUp>\n  );\n};\n\nconst mapStateToProps = (state) => {\n  const data = getOrderedPizzas(state);\n  return { data };\n};\n\nexport default connect(mapStateToProps)(Cart);\n","/home/waldemar/Documents/React/pizza-place/src/popUp/PopUp.js",[],"/home/waldemar/Documents/React/pizza-place/src/redux/store.js",[],"/home/waldemar/Documents/React/pizza-place/src/redux/constants/actionTypes.js",[],"/home/waldemar/Documents/React/pizza-place/src/redux/reducers/index.js",[],"/home/waldemar/Documents/React/pizza-place/src/redux/reducers/popUp.js",["128"],"import { OPEN_POPUP, CLOSE_POPUP } from \"../constants/actionTypes\";\n\nconst initialState = { openedPopUpsIds: [] };\n\nconst reducer = (state = initialState, action) => {\n  switch (action.type) {\n    case OPEN_POPUP:\n      //   console.log(\"Reducer: popUp: OPEN_POPUP \", action.payload.id);\n      let isIdExisting = state.openedPopUpsIds.indexOf(action.payload.id) > -1;\n      let openedPopUpsIds = { ...state }.openedPopUpsIds;\n\n      if (!isIdExisting) {\n        return {\n          ...state,\n          openedPopUpsIds: [...state.openedPopUpsIds, action.payload.id],\n        };\n      }\n\n      return {\n        state,\n      };\n    case CLOSE_POPUP:\n      //   console.log(\"Reducer: popUp: CLOSE_POPUP \", action.payload.id);\n      return {\n        ...state,\n        openedPopUpsIds: state.openedPopUpsIds.filter(\n          (id) => id !== action.payload.id\n        ),\n      };\n    default:\n      //   console.log(\"Reducer: popup: default\");\n      return state;\n  }\n};\n\nexport default reducer;\n","/home/waldemar/Documents/React/pizza-place/src/redux/constants/popUpIds.js",[],"/home/waldemar/Documents/React/pizza-place/src/redux/selectors/popUp.js",[],"/home/waldemar/Documents/React/pizza-place/src/redux/actions/popUp.js",[],"/home/waldemar/Documents/React/pizza-place/src/redux/reducers/pizza.js",[],"/home/waldemar/Documents/React/pizza-place/src/redux/actions/pizza.js",[],"/home/waldemar/Documents/React/pizza-place/src/redux/constants/url.js",[],"/home/waldemar/Documents/React/pizza-place/src/redux/reducers/order.js",[],"/home/waldemar/Documents/React/pizza-place/src/redux/actions/order.js",[],"/home/waldemar/Documents/React/pizza-place/src/redux/selectors/order.js",[],"/home/waldemar/Documents/React/pizza-place/src/redux/selectors/pizza.js",[],"/home/waldemar/Documents/React/pizza-place/src/utils/priceToDisplay.js",["129"],"export default (price) => {\n  return (price / 100).toFixed(2);\n};\n","/home/waldemar/Documents/React/pizza-place/src/redux/reducers/extraTopping.js",[],"/home/waldemar/Documents/React/pizza-place/src/redux/actions/extraToppings.js",[],{"ruleId":"130","replacedBy":"131"},{"ruleId":"132","replacedBy":"133"},{"ruleId":"134","severity":1,"message":"135","line":6,"column":17,"nodeType":"136","messageId":"137","endLine":6,"endColumn":25},{"ruleId":"134","severity":1,"message":"138","line":10,"column":7,"no